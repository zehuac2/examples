@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container">
    <div class="row">
        <div class="col list-group" role="list">
            @foreach (var example in Model.Examples)
            {
                <a
                   class="list-group-item list-group-item-action"
                   asp-page="@example.Url"
                   role="listitem"
                >
                    @example.Name
                </a>
            }
        </div>
    </div>
</div>
